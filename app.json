      },
      "bundleIdentifier": "com.trudeausac.pethsapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7925a464-a7a6-435b-8852-f8b67f661ac7"
      }
    }
  }
}
